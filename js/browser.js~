function back() {
	
	document.getElementById("iframe").src=history.go(-1);
	
}

function forward() {
	
	document.getElementById("iframe").src=history.go(1);
	
}

function refresh() {
	
	document.getElementById("iframe").src=history.go(0);
	
}

function go() {
	
	document.getElementById("iframe").src=document.getElementById("url").value;
	
}